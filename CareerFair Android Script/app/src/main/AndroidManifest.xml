<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reddy.careerfair">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Student_login" />
        <activity android:name=".Employer_login" />
        <activity android:name=".student_register" />
        <activity android:name=".employer_register" />
        <activity android:name=".Student_update" />
        <activity android:name=".Student_dashboard" />
        <activity android:name=".comp_data"></activity>
    </application>

</manifest>